 stage('RA Deploy') {
      steps {
	        script {
	          echo "Starting Deploy"
              runDeploy(modules, REPO_NAME)
	        }
	      }
	    } 






def runDeploy(modulelist, repo_name) {
  BRANCH_NAME = BRANCH_NAME.replaceAll('/', '_')
  for (int i = 0; i < modulelist.size(); i++) {
    echo "Running DEPLOY for ${modulelist[i]}"
    def values=modulelist[i].split('/')
    LOB=values[0]
    APPLICATION=values[1]
    echo "Call RA Deploy Job with Params: REPO_NAME=${repo_name} LOB=${LOB} APPLICATION=${APPLICATION} RELEASE_VERSION=${RELEASE_VERSION} ENV=${TARGET_ENV} BRANCH_NAME=${BRANCH_NAME}"

    build job: "${DEPLOY_JOB}",
    parameters: [
        string(name: 'REPO_NAME', value: "${repo_name}"),
        string(name: 'LOB', value: "${LOB}"),
        string(name: 'APPLICATION', value: "${APPLICATION}"),
        string(name: 'RELEASE_VERSION', value: "${RELEASE_VERSION}"),
        string(name: 'ENV', value: "${TARGET_ENV}"),
        string(name: 'KEY', value: "${repo_name}_${BRANCH_NAME}")
    ]
  }
} 
 








